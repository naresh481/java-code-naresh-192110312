import java.util.*;
public class belowpattern
{
  public staic void main(String[]args)
  {
  Scanner s = new Scaneer(system.in);
  System.out.println("Enter the Nth term"N"");
  int n= s.nextint();
  for(int i = 1; i < =n; i++)
  {
    for(intj = 1; j<=i;j++)
	{
	  System.out.print(i);
	}
	System.out.print(" ");
	}
   }
 }
